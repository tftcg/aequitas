<!doctype html>
<html lang="en">
<!-- THIS IS A GENERATED FILE. DO NOT EDIT THIS -->
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>{{ faq_node.attrib['name'] }}</title>
  <meta property="og:type" content="website"/>
  <meta property="og:title" content="{{ title }}"/>
  <meta property="og:description" content="Aequitas - Transformers TCG Rules"/>
  <meta property="og:url" content="https://tftcg.info/{{ filename }}"/>
  <meta property="og:image" content="https://faq.tf-rules.info/images/Aequitas-logo.jpg"/>
  <meta property="og:image:alt" content="Aequitas Logo"/>
  <meta property="og:image:width" content="200"/>
  <meta property="og:image:height" content="200"/>
  <link rel="stylesheet" type="text/css" href="/style/style.css"/>
</head>
<body>
    <div id="sitetitle">
        <img id="logo" style="padding: 0.25em;" src="/images/Aequitas-logo.jpg"/>
        <span id="title_widget">
            Aequitas FAQ
        </span>
    </div>


    <!--
    # <faq name source source_url published_date sourced_at>
    #   <target name>
    #     <entry id>
    #       <question>
    #       <answer>
    -->

    <h3 class="card_title">{{ faq_node.attrib['name'] }}</h3>

    <dl class="card_faqs">

{% for target in faq_node.findall('target') %}
    <dt>{{ target.attrib['name'] }}</li>
    <dd>
    <ul class="card_faq">
  {% for entry in target.findall('entry') %}
    <li id="{{ faq_node.attrib['name'] }}__{{ entry.attrib['id'] }}" class="faq_entry">
      <div class="question">
        <a name="{{ faq_node.attrib['source'] }}__{{ entry.attrib['id'] }}"/>
        {{ f_prepare_text(entry.find('question')) }}
        <span><a href="#{{ faq_node.attrib['source'] }}__{{ entry.attrib['id'] }}"><img src="/images/permalink.svg"/></a></span>
      </div>
      <div class="answer">
        {{ f_prepare_text(entry.find('answer')) }}
      </div>
    </li>
  {% endfor %}
    </ul>
    </dd>
{% endfor %}

    </dl>

</body>
</html>
